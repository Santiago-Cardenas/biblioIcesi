{
  "name": "crud-node",
  "version": "0.0.1",
  "description": "Crud example",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPatterns=integration",
    "dev": "nodemon ./src/index.ts",
    "seed": "node -r dotenv/config -r ts-node/register scripts/seed.ts",
    "ensure-indexes": "node -r dotenv/config -r ts-node/register scripts/ensure-indexes.ts",
    "create-admin": "tsx src/scripts/createAdmin.ts",
    "migrate": "node scripts/migrate.js"
  },
  "author": "VaSaMo",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.17.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.2",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.10",
    "serverless-http": "^4.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
